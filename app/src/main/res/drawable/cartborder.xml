<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid
                android:color="#992C04"/>

            <corners
                android:radius="6dp"/>

        </shape>

    </item>
    <item>
        <shape>
            <gradient
                android:angle="270"
                android:startColor="#992C04"
                android:endColor="#992C04"/>
            <stroke
                android:width="2dp"
                android:color="#992C04"/>
            <corners
                android:radius="5dp"/>

        </shape>
    </item>
</selector>